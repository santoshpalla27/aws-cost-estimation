# Pricing Miner Environment Configuration

# AWS Pricing API
AWS_PRICING_BASE_URL=https://pricing.us-east-1.amazonaws.com
AWS_OFFER_INDEX_PATH=/offers/v1.0/aws/index.json
AWS_REQUEST_TIMEOUT=300000
AWS_MAX_RETRIES=3

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=pricing
DB_USER=postgres
DB_PASSWORD=postgres
DB_MAX_CONNECTIONS=10

# Ingestion
INGESTION_CONCURRENCY=3
INGESTION_BATCH_SIZE=10000
STREAM_HIGH_WATER_MARK=65536

# Logging
LOG_LEVEL=info
LOG_PRETTY=true
